                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH XMLNAMESPACES (DEFAULT 'http://schemas.microsoft.com/sqlserver/2004/07/adventure-works/IndividualSurvey')
SELECT p.BusinessEntityID, 
c.AccountNumber,
p.NameStyle,
p.Title, 
p.FirstName, 
p.MiddleName, 
p.LastName, 
p.Suffix, 
pp.PhoneNumber, 
pnt.Name AS PhoneNumberType, 
ea.EmailAddress, 
p.EmailPromotion, 
at.Name AS AddressType, 
a.AddressLine1, a.AddressLine2, 
a.City,
sp.StateProvinceCode,
sp.Name AS StateProvinceName, 
a.PostalCode, 
cr.CountryRegionCode,
cr.Name AS CountryRegionName, 
p.Demographics.value('(IndividualSurvey/MaritalStatus)[1]', 'NCHAR(1)') as MaritalStatus,
p.Demographics.value('(IndividualSurvey/DateFirstPurchase)[1]', 'DATE') as DateFirstPurchase,
p.Demographics.value('(IndividualSurvey/BirthDate)[1]', 'DATE') as BirthDate,
p.Demographics.value('(IndividualSurvey/Gender)[1]', 'nvarchar(1)') as Gender,
p.Demographics.value('(IndividualSurvey/TotalChildren)[1]', 'tinyint') as TotalChildren,
p.Demographics.value('(IndividualSurvey/NumberChildrenAtHome)[1]', 'tinyint') as NumberChildrenAtHome,
p.Demographics.value('(IndividualSurvey/Education)[1]', 'nvarchar(40)') as Education,
p.Demographics.value('(IndividualSurvey/Occupation)[1]', 'nvarchar(100)') as Occupation,
p.Demographics.value('(IndividualSurvey/HomeOwnerFlag)[1]', 'nchar(1)') as HomeOwnerFlag,
p.Demographics.value('(IndividualSurvey/NumberCarsOwned)[1]', 'tinyint') as NumberCarsOwned,
p.Demographics.value('(IndividualSurvey/CommuteDistance)[1]', 'nvarchar(15)') as CommuteDistance
FROM     Person.Person AS p INNER JOIN
                  Person.BusinessEntityAddress AS bea ON bea.BusinessEntityID = p.BusinessEntityID INNER JOIN
                  Person.Address AS a ON a.AddressID = bea.AddressID INNER JOIN
                  Person.StateProvince AS sp ON sp.StateProvinceID = a.StateProvinceID INNER JOIN
                  Person.CountryRegion AS cr ON cr.CountryRegionCode = sp.CountryRegionCode INNER JOIN
                  Person.AddressType AS at ON at.AddressTypeID = bea.AddressTypeID INNER JOIN
                  Sales.Customer AS c ON c.PersonID = p.BusinessEntityID LEFT OUTER JOIN
                  Person.EmailAddress AS ea ON ea.BusinessEntityID = p.BusinessEntityID LEFT OUTER JOIN
                  Person.PersonPhone AS pp ON pp.BusinessEntityID = p.BusinessEntityID LEFT OUTER JOIN
                  Person.PhoneNumberType AS pnt ON pnt.PhoneNumberTypeID = pp.PhoneNumberTypeID
WHERE  (c.StoreID IS NULL and bea.AddressTypeID = 2)</property>
