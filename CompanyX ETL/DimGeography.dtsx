    Person.Address AS A
INNER JOIN
    Person.StateProvince AS SP ON A.StateProvinceID = SP.StateProvinceID
INNER JOIN
    Person.CountryRegion AS CR ON SP.CountryRegionCode = CR.CountryRegionCode</property>
